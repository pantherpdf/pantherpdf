{
  "name": "pantherpdf",
  "version": "4.0.0-beta.3",
  "keywords": ["report", "editor", "pdf", "wysiwyg"],
  "homepage": "https://www.pantherpdf.com/",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "esmodule": "dist/index.esmodule.js",
  "umd:main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "umd": "./dist/index.umd.js",
      "import": "./dist/index.module.js",
      "require": "./dist/index.js",
      "default": "./dist/index.module.js"
    }
  },
  "files": [
    "dist/*.{js,mjs,ts,map,css}",
    "README.md",
    "package.json"
  ],
  "sideEffects": false,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "file-saver": "^2.0.5",
    "react-bootstrap": "^2.0.3",
    "react-colorful": "^5.5.1",
    "react-style-object-to-css": "^1.1.2"
  },
  "peerDependencies": {
    "bootstrap": ">= 5.1.0 < 6",
    "react": ">= 16.8.0 < 19",
    "react-dom": ">= 16.8.0 < 19"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@parcel/packager-ts": "latest",
    "@parcel/transformer-sass": "latest",
    "@parcel/transformer-typescript-types": "latest",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/file-saver": "^2.0.4",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "babel-jest": "^29.5.0",
    "bootstrap": "^5.1.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-serializer-html": "^7.1.0",
    "parcel": "latest",
    "prettier": "^2.8.4",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.4.3",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "scripts": {
    "build": "parcel build",
    "start": "parcel serve demo/index.html --public-url / --dist-dir build",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "npm run eslint src"
  },
  "browserslist": {
    "production": [
      ">10%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pantherpdf/pantherpdf.git"
  }
}
