{
	"name": "reports-backend",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"start": "netlify-lambda serve src/functions --config ./webpack.functions.js",
		"build": "netlify-lambda build src/functions --config ./webpack.functions.js"
	},
	"dependencies": {
		"@netlify/functions": "^0.7.2",
		"encoding": "^0.1.13",
		"netlify-lambda": "^2.0.8",
		"node-fetch": "^2.6.1"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.14.5",
		"@babel/preset-typescript": "^7.14.5",
		"@types/node-fetch": "^2.5.12",
		"css-loader": "^4.3.0",
		"dotenv-webpack": "7.0.3",
		"typescript": "^4.3.2"
	},
	"comments": [
		"package encoding: ^0.1.13 is not directly required but building on netlify fails without it. Maybe because of zip-it-and-ship-it",
		"netlify-lambda is using old webpack version 4. css-loader must have older version."
	],
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-typescript"
		]
	}
}
